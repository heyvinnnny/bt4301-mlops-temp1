{"ast":null,"code":"export default {\n  name: \"paper-table\",\n  props: {\n    columns: Array,\n    data: Array,\n    type: {\n      type: String,\n      // striped | hover\n      default: \"striped\"\n    },\n    title: {\n      type: String,\n      default: \"\"\n    },\n    subTitle: {\n      type: String,\n      default: \"\"\n    }\n  },\n  computed: {\n    tableClass: function tableClass() {\n      return \"table-\".concat(this.type);\n    }\n  },\n  methods: {\n    hasValue: function hasValue(item, column) {\n      return item[column.toLowerCase()] !== \"undefined\";\n    },\n    itemValue: function itemValue(item, column) {\n      return item[column.toLowerCase()];\n    }\n  }\n};","map":{"version":3,"names":["name","props","columns","Array","data","type","String","default","title","subTitle","computed","tableClass","concat","methods","hasValue","item","column","toLowerCase","itemValue"],"sources":["src/components/PaperTable.vue"],"sourcesContent":["<template>\r\n  <table class=\"table\" :class=\"tableClass\">\r\n    <thead>\r\n      <slot name=\"columns\">\r\n        <th v-for=\"column in columns\" :key=\"column\">{{ column }}</th>\r\n      </slot>\r\n    </thead>\r\n    <tbody>\r\n      <tr v-for=\"(item, index) in data\" :key=\"index\">\r\n        <slot :row=\"item\">\r\n          <td\r\n            v-for=\"(column, index) in columns\"\r\n            :key=\"index\"\r\n            v-if=\"hasValue(item, column)\"\r\n          >\r\n            {{ itemValue(item, column) }}\r\n          </td>\r\n        </slot>\r\n      </tr>\r\n    </tbody>\r\n  </table>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"paper-table\",\r\n  props: {\r\n    columns: Array,\r\n    data: Array,\r\n    type: {\r\n      type: String, // striped | hover\r\n      default: \"striped\",\r\n    },\r\n    title: {\r\n      type: String,\r\n      default: \"\",\r\n    },\r\n    subTitle: {\r\n      type: String,\r\n      default: \"\",\r\n    },\r\n  },\r\n  computed: {\r\n    tableClass() {\r\n      return `table-${this.type}`;\r\n    },\r\n  },\r\n  methods: {\r\n    hasValue(item, column) {\r\n      return item[column.toLowerCase()] !== \"undefined\";\r\n    },\r\n    itemValue(item, column) {\r\n      return item[column.toLowerCase()];\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"],"mappings":"AAuBA;EACAA,IAAA;EACAC,KAAA;IACAC,OAAA,EAAAC,KAAA;IACAC,IAAA,EAAAD,KAAA;IACAE,IAAA;MACAA,IAAA,EAAAC,MAAA;MAAA;MACAC,OAAA;IACA;IACAC,KAAA;MACAH,IAAA,EAAAC,MAAA;MACAC,OAAA;IACA;IACAE,QAAA;MACAJ,IAAA,EAAAC,MAAA;MACAC,OAAA;IACA;EACA;EACAG,QAAA;IACAC,UAAA,WAAAA,WAAA;MACA,gBAAAC,MAAA,MAAAP,IAAA;IACA;EACA;EACAQ,OAAA;IACAC,QAAA,WAAAA,SAAAC,IAAA,EAAAC,MAAA;MACA,OAAAD,IAAA,CAAAC,MAAA,CAAAC,WAAA;IACA;IACAC,SAAA,WAAAA,UAAAH,IAAA,EAAAC,MAAA;MACA,OAAAD,IAAA,CAAAC,MAAA,CAAAC,WAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}